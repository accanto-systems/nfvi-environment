apiVersion: v1
baseDomain: {{ network_domain }}
compute:
- hyperthreading: Enabled
  name: worker
  replicas: 0
controlPlane:
  hyperthreading: Enabled
  name: master
  replicas: 1
metadata:
  name: {{ openshift_domain }}
networking:
  clusterNetworks:
  - cidr: {{ openshift_clusternet }}
    hostPrefix: {{ openshift_clusternetprefix }}
  networkType: OpenShiftSDN
  serviceNetwork:
  - {{ openshift_servicenet }}
platform:
  none: {}
pullSecret: "{{ openshift_pull_secret }}"
sshKey: "{{ openshift_ssh_key }}"